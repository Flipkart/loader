{

    "chartResources":{
        "mysql":{
            "resource":"mysql",
            "charts":[
                {
                    "title":"Threads",
                    "keysToPlot":[
                        "Threads_running",
                        "Threads_cached",
                        "Threads_connected",
                        "Connections"
                    ]
                },
                {
                    "title":"Table_Status",
                    "keysToPlot":[
                        "Com_lock_tables",
                        "Com_stmt_execute",
                        "Table_locks_waited",
                        "Innodb_deadlocks"
                    ]
                },
                {
                    "title":"QueriesAndData",
                    "keysToPlot":[
                        "Slow_queries",
                        "Queries",
                        "Bytes_sent",
                        "Bytes_received"
                    ]
                }
            ]
        },
        "cpu.total":{
            "resource":"cpu.total",
            "charts":[
                {
                    "title":"CPU",
                    "keysToPlot":[
                        "system",
                        "user",
                        "idle"
                    ]
                }
            ]
        },
        "sockets":{
            "resource":"sockets",
            "charts":[
                {
                    "title":"Mem",
                    "keysToPlot":[
                        "tcp_mem",
                        "udp_mem"
                    ]
                },
                {
                    "title":"Sockets In Use",
                    "keysToPlot":[
                        "tcp_inuse",
                        "udp_inuse"
                    ]
                }
            ]
        },
        "diskspace.root":{
            "resource":"diskspace.root",
            "charts":[
                {
                    "title":"Diskspace",
                    "keysToPlot":[
                        "byte_used",
                        "byte_free"
                    ]
                }
            ]
        },
        "jmx":{
            "resource":"jmx",
            "charts":[
                {
                    "title":"Memory",
                    "keysToPlot":[
                        "memory.heapMemory.usagePercentage",
                        "memory.nonHeapMemory.usagePercentage"
                    ]
                },
                {
                    "title":"SystemLoad",
                    "keysToPlot":[
                        "os.systemAverageLoad"
                    ]
                },
                {
                    "title":"GCCount",
                    "keysToPlot":[
                        "gc.ParNew.count",
                        "gc.ConcurrentMarkSweep.count"
                    ]
                }
            ]
        },
        "memory":{
            "resource":"memory",
            "charts":[
                {
                    "title":"Physical Memory",
                    "keysToPlot":[
                        "MemTotal",
                        "MemFree",
                        "Cached",
                        "Active",
                        "Buffers",
                        "Dirty"
                    ]
                },
                {
                    "title":"Swap",
                    "keysToPlot":[
                        "SwapTotal",
                        "SwapFree",
                        "SwapCached"
                    ]
                }
            ]
        }
    }

}